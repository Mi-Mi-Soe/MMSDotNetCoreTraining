<form action="/PaintingHttpClient/Save" method="post">
    <div class="mb-3">
        <label class="form-label">Painting Name</label>
        <input type="text" class="form-control" name="PaintingName" placeholder="name@example.com">
    </div>
    <div class="mb-3">
        <label class="form-label">Painting Type</label>
        <input type="text" class="form-control" name="PaintingType" placeholder="name@example.com">
    </div>
    <div class="mb-3">
        <label class="form-label">Painting Price</label>
        <input type="text" class="form-control" name="PaintingPrice" placeholder="name@example.com">
    </div>
    <a href="/PaintingHttpClient/Index" class="btn btn-outline-dark">Back</a>
    <button type="submit" class="btn btn-success">Save</button>
</form>
